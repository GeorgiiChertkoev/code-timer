{
  "name": "code-timer",
  "displayName": "files-timer",
  "description": "counts time you spend on file",
  "icon": "images/logo.png",
  "version": "0.1.4",
  "publisher": "geochert",
  "engines": {
    "vscode": "^1.91.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GeorgiiChertkoev/code-timer"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ], 
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "code-timer.showFileTimes",
        "title": "Show Time Files Were Open"
      }, 
      {
        "command": "code-timer.eraseAllTimes",
        "title": "Clear all written time"
      }, 
      {
        "command": "code-timer.eraseOneFileTime",
        "title": "Clear one file time"
      }

    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.91.1",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
